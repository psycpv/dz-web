{
  "name": "zwirnerweb",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "template",
    "nextjs"
  ],
  "license": "MIT",
  "main": "package.json",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next",
    "format": "npx prettier --write .",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "start": "next start",
    "test": "yarn type-check && yarn jest --runInBand",
    "test-all": "yarn type-check && yarn lint:fix && yarn format",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit --pretty"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@headlessui/tailwindcss": "^0.1.2",
    "@next/font": "^13.4.3",
    "@portabletext/react": "^2.0.0",
    "@sanity/client": "^6.0.1",
    "@sanity/image-url": "^1.0.2",
    "@tailwindcss/typography": "^0.5.9",
    "@zwirner/design-system": "0.0.10",
    "axios": "^1.3.5",
    "next": "^13.1.2",
    "next-recaptcha-v3": "^1.1.5",
    "next-sanity": "^4.3.2",
    "next-seo": "^5.15.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-hook-form": "^7.43.9",
    "styled-components": "^5.3.10",
    "swr": "^2.1.1",
    "tailwind-scrollbar": "^2.1.0",
    "uuidv4": "^6.2.13"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.3.1",
    "@playwright/test": "^1.34.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^20.2.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.2.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-next": "13.1.2",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.0",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.5.1",
  "publishConfig": {
    "@zwirner:registry": "https://npm.pkg.github.com"
  }
}

{
  "name": "zwirnerweb",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "template",
    "nextjs"
  ],
  "license": "MIT",
  "main": "package.json",
  "scripts": {
    "analyze": "cross-env ANALYZE=true yarn build",
    "build": "next build",
    "dev": "next dev",
    "format": "npx prettier --write .",
    "postinstall": "husky install",
    "lint": "next lint -- --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --check .",
    "start": "next start",
    "test": "yarn type-check && yarn jest --runInBand",
    "test-all": "yarn type-check && yarn lint:fix && yarn format",
    "test:e2e": "playwright test",
    "type-check": "tsc --noEmit --pretty"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@headlessui/tailwindcss": "^0.1.2",
    "@heroicons/react": "^2.0.18",
    "@portabletext/react": "^3.0.11",
    "@sanity/image-url": "^1.0.2",
    "@sanity/preview-url-secret": "^1.3.1",
    "@sanity/webhook": "^3.0.1",
    "@sentry/nextjs": "^7.85.0",
    "@shopify/hydrogen-react": "^2023.7.6",
    "@t3-oss/env-nextjs": "^0.6.1",
    "@tailwindcss/typography": "^0.5.9",
    "@zwirner/design-system": "v0.0.187",
    "axios": "^1.3.5",
    "classnames": "^2.3.2",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "next": "^13.5.6",
    "next-sanity": "^7.0.1",
    "next-seo": "^5.15.0",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.43.9",
    "react-is": "18.2.0",
    "react-toast-notifications": "^2.5.1",
    "react-visibility-sensor": "^5.1.1",
    "sanity-plugin-iframe-pane": "^3.1.2",
    "styled-components": "^5.3.10",
    "swr": "^2.1.1",
    "tailwind-scrollbar": "^2.1.0",
    "uuidv4": "^6.2.13",
    "zod": "^3.22.2",
    "zustand": "4.4.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.3.1",
    "@playwright/test": "^1.34.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.2.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.2.4",
    "@types/react-google-recaptcha": "^2.1.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "autoprefixer": "^10.4.16",
    "checkly": "^4.1.0",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "eslint": "^8.53.0",
    "eslint-config-next": "13.5.6",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lodash": "^4.17.21",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-packagejson": "^2.4.6",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "tailwindcss": "^3.2.4",
    "turbo": "^1.10.16",
    "typescript": "^4.9.4",
    "zukeeper": "^1.0.2"
  },
  "packageManager": "yarn@3.5.1",
  "publishConfig": {
    "@zwirner:registry": "https://npm.pkg.github.com"
  }
}
